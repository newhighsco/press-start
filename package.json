{
  "name": "@newhighsco/press-start",
  "description": "Chipset + Next.js site starter by New High Score",
  "version": "1.3.1",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/press-start.git"
  },
  "homepage": "https://github.com/newhighsco/press-start#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/press-start/issues"
  },
  "scripts": {
    "start": "next dev",
    "build": "next build && next export --outdir build",
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.jsx,.json .",
    "lint:css": "stylelint '**/*.{css,scss}' --cache",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix",
    "format:css": "npm run lint:css -- --fix"
  },
  "dependencies": {
    "@newhighsco/chipset": "1.6.0",
    "@newhighsco/next-plugin-sitemap": "1.0.0",
    "next": "9.3.4",
    "next-compose-plugins": "2.2.0",
    "next-ga": "2.3.4",
    "next-images": "1.4.0",
    "next-seo": "4.4.0",
    "next-transpile-modules": "3.2.0",
    "node-sass": "4.13.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sitemap": "6.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@newhighsco/browserslist-config": "1.0.0",
    "@newhighsco/commitlint-config": "1.0.0",
    "@newhighsco/editor-config": "1.1.0",
    "@newhighsco/eslint-config": "2.0.6",
    "@newhighsco/prettier-config": "2.0.0",
    "@newhighsco/release-config": "1.0.19",
    "@newhighsco/renovate-config": "1.2.0",
    "@newhighsco/stylelint-config": "2.0.5",
    "eslint": "6.8.0",
    "husky": "4.2.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.4",
    "semantic-release": "17.0.4",
    "stylelint": "13.3.0"
  },
  "browserslist": {
    "extends": [
      "@newhighsco/browserslist-config"
    ]
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config/react"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config"
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
