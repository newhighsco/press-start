{
  "name": "@newhighsco/press-start",
  "description": "Chipset + Next.js site starter by New High Score",
  "version": "1.18.3",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/press-start.git"
  },
  "homepage": "https://github.com/newhighsco/press-start#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/press-start/issues"
  },
  "scripts": {
    "clean": "rm -rf {.next,build}",
    "start": "next dev",
    "build": "next build && next export --outdir build",
    "postbuild": "next-sitemap",
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.json,.jsx,.mdx .",
    "lint:css": "stylelint '**/*.{css,scss}' --cache",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix",
    "format:css": "npm run lint:css -- --fix"
  },
  "dependencies": {
    "@newhighsco/chipset": "6.2.0",
    "@newhighsco/next-plugin-svgr": "3.0.1",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-optipng": "8.0.0",
    "next": "9.5.3",
    "next-compose-plugins": "2.2.0",
    "next-fonts": "1.4.0",
    "next-ga": "2.3.4",
    "next-optimized-images": "2.6.2",
    "next-seo": "4.7.3",
    "next-sitemap": "1.2.4",
    "next-transpile-modules": "4.1.0",
    "next-videos": "1.3.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sass": "1.26.10"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@newhighsco/browserslist-config": "1.0.2",
    "@newhighsco/commitlint-config": "1.0.6",
    "@newhighsco/editor-config": "1.1.1",
    "@newhighsco/eslint-config": "2.2.24",
    "@newhighsco/postcss-config": "2.0.3",
    "@newhighsco/prettier-config": "2.0.5",
    "@newhighsco/release-config": "1.0.26",
    "@newhighsco/renovate-config": "1.4.0",
    "@newhighsco/stylelint-config": "2.0.21",
    "eslint": "7.8.1",
    "husky": "4.2.5",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.1",
    "semantic-release": "17.1.1",
    "stylelint": "13.7.0"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config/react"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config"
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
